<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
				   alpha.disabled="0.5">
	
	<fx:Metadata>
		<![CDATA[ 
		/** 
		* @copy spark.skins.spark.ApplicationSkin#hostComponent
		*/
		[HostComponent("spark.components.Button")]
		]]>
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	
	<s:Group width="100%" height="100%" verticalCenter="0" horizontalCenter="0">
		<s:Ellipse width="100%" height="100%" horizontalCenter="0" verticalCenter="0"
				   top.down="1">
			<s:stroke>
				<s:LinearGradientStroke>
					<s:GradientEntry color="#8fafc9" ratio="0"/>
					<s:GradientEntry color="#8bb2d2" ratio="1"/>
				</s:LinearGradientStroke>
			</s:stroke>
			<s:fill>
				<s:LinearGradient>
					<s:GradientEntry color.disabled="#cccfd1"
									 color.down="#c3d9e5"
									 color.over="#d2eaf8"
									 color.up="#e0e7eb"/>
				</s:LinearGradient>
			</s:fill>
		</s:Ellipse>
		<s:Path width="9" height="9"
				data="M0 3.75977 3.74023 3.75977 3.74023 0 6.89453 0 6.89453 3.75977 10.6543 3.75977 10.6543 6.91406 6.89453 6.91406 6.89453 10.6543 3.74023 10.6543 3.74023 6.91406 0 6.91406 0 3.75977Z"
				horizontalCenter="0" verticalCenter="0"
				top.down="1">
			<s:fill>
				<s:SolidColor color="0x111419"/>
			</s:fill>
			<s:filters>
				<!--<s:DropShadowFilter color="#000000" angle="-90" distance="1"/>-->
				<s:GlowFilter blurX="40" blurY="40" color="#ffffff" quality="99"/>
			</s:filters>
		</s:Path>
		
	</s:Group>
	
	
</s:SparkButtonSkin>
