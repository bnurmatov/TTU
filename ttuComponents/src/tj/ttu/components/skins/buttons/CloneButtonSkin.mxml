<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="25" height="26">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage left="0" top="0" smooth="true" 
				   verticalCenter="0" horizontalCenter="0"
				   source="@Embed('/embed_assets/images/reuse_content_icon.png')"
				   alpha.disabled="0.34"
				   top.down="1">
		<s:filters>
			<s:GlowFilter includeIn="over" alpha="1.0" blurX="5" blurY="5" color="#5380d0"
						  inner="false" knockout="false" quality="2" strength="1"/>
		</s:filters>
	</s:BitmapImage>
</s:Skin>
